    <br>
      <ul class="list-group">
        <tr class="list-group-item"><b>Queste sono le informazioni del tuo Parcheggio:</b> </tr>
        <li class="list-group-item"> AUTO : <%- parcheggio.mezzo_auto %> </li>
        <li class="list-group-item"> MOTO : <%- parcheggio.mezzo_moto %>  </li>
        <li class="list-group-item"> BICI : <%- parcheggio.mezzo_bici %>  </li>
        <li class="list-group-item"> MONOPATTINI : <%- parcheggio.mezzo_monopattino %> </li>
        <li class="list-group-item"> NUMERO AUTO : <%- parcheggio.numero_auto %>  </li>
        <li class="list-group-item"> NUMERO MOTO : <%- parcheggio.numero_moto %>  </li>
        <li class="list-group-item"> NUMERO BICI : <%- parcheggio.numero_bici %>  </li>
        <li class="list-group-item"> NUMERO MONOPATTINI : <%- parcheggio.numero_monopattino %> </li>
        <li class="list-group-item"> STATO : <%- parcheggio.stato %> </li>
        <li class="list-group-item"> DESCRIZIONE : <%- parcheggio.descrizione %> </li>
        <li class="list-group-item"> FOTO : <%- parcheggio.foto %> </li>

        <div class="img-wrapper"><img style="width : 200px; height : 200px;" src="../images/<%-parcheggio.foto%>" alt="image Alt" title="Image Title"></div>


      <div role="group" aria-label="Basic example" style="text-align: right; padding-top: 15px">
          <a href="modificadatiParcheggio"><button type="button" class="btn btn-outline-secondary">Modifica Dati Parcheggio</button></a>
          <a href="/users/eliminautente" onclick="if(!confirm('Sei sicuro di voler eliminare?')) return false"><button type="button" class="btn btn-outline-secondary">Elimina Account</button></a>
      </div>   <br>
      <h3>Noleggi effettuati</h3>


    


<script>
notifica = ('<%-notifica.string%>')
counter = parseInt('<%-notifica.counter%>')



if (notifica == 'parcheggio modificato' && counter == 0 ){

const Toast = Swal.mixin({
  toast: true,
  position: 'top-end',
  showConfirmButton: false,
  timer: 3000,
  timerProgressBar: true,
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer)
    toast.addEventListener('mouseleave', Swal.resumeTimer)
  }
})
var state = history.state || {};
var reloadCount = state.reloadCount || 0;
if (performance.navigation.type === 1) { // Reload
state.reloadCount = ++reloadCount;
history.replaceState(state, null, document.URL);
} else if (reloadCount) {
delete state.reloadCount;
reloadCount = 0;
history.replaceState(state, null, document.URL);
}
if (reloadCount >= 1) {
// Now, do whatever you want...
}
else{                   
Toast.fire({
  icon: 'success',
  title: 'Parcheggio modificato con successo'
})}


}

      </script>